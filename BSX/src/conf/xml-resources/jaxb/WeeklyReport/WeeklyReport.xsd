						<xs:sequence>
							
							<xs:element name="OtherReceiptKHR" type="xs:decimal" nillable="true"/>
							<xs:element name="OtherReceiptRemark" type="xs:string" nillable="true"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="TotalReceipt">
					<xs:complexType>
						<xs:sequence>
							
							<xs:element name="TotalReceiptKHR" type="xs:decimal" nillable="true"/>
							<xs:element name="TotalReceiptRemark" type="xs:string" nillable="true"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="SubTotalReceipt">
					<xs:complexType>
						<xs:sequence>
							
							<xs:element name="SubTotalReceiptKHR" type="xs:decimal" nillable="true"/>
							<xs:element name="SubTotalReceiptRemark" type="xs:string" nillable="true"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
