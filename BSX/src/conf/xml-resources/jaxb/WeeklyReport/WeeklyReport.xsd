					</xs:simpleType>
				</xs:element>
				<xs:element name="ReportingDate" type="xs:date" nillable="true"/>
				<xs:element name="ExchangeRate" type="xs:decimal" nillable="true"/>
				<xs:element name="PreviousBalance">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="PreviousBalanceKHR" type="xs:decimal" nillable="true"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="FromDeposit">
					<xs:complexType>
						<xs:sequence>
						
							<xs:element name="FromDepositKHR" type="xs:decimal" nillable="true"/>
							<xs:element name="FromDepositRemark" type="xs:string" nillable="true"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="FDSaving">
