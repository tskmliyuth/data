						<xs:sequence>
						
							<xs:element name="FDSavingKHR" type="xs:decimal" nillable="true"/>
							<xs:element name="FDSavingRemark" type="xs:string" nillable="true"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="FDCurrent">
