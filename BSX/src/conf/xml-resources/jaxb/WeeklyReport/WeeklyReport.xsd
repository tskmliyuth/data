						<xs:sequence>
							
							<xs:element name="FDCurrentKHR" type="xs:decimal" nillable="true"/>
							<xs:element name="FDCurrentRemark" type="xs:string" nillable="true"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="FDFixed">
					<xs:complexType>
						<xs:sequence>
							
							<xs:element name="FDFixedKHR" type="xs:decimal" nillable="true"/>
							<xs:element name="FDFixedRemark" type="xs:string" nillable="true"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="LoanRepayments">
					<xs:complexType>
