							<xs:element name="FundToOtherBankKHR" type="xs:decimal" nillable="true"/>
							<xs:element name="FundToOtherBankRemark" type="xs:string" nillable="true"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="OtherPayment">
